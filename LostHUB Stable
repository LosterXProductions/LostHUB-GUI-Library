GUI.CreateButton = function(Name, func)
	-- Instances:

	local Button = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local Content = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local TextLabel = Instance.new("TextLabel")
	local Logo = Instance.new("ImageLabel")

	--Properties:

	Button.Name = Name
	Button.Parent = CurrentContent
	Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Button.BackgroundTransparency = 1.000
	Button.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Button.BorderSizePixel = 0
	Button.Position = UDim2.new(0.325737268, 0, 6.07363333e-08, 0)
	Button.Size = UDim2.new(0, 280, 0, 60)

	UICorner.CornerRadius = UDim.new(0, 10)
	UICorner.Parent = Button

	Content.Name = "Content"
	Content.Parent = Button
	Content.AnchorPoint = Vector2.new(0.5, 0.5)
	Content.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Content.BackgroundTransparency = 0.800
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(0.5, 0, 0.5, 0)
	Content.Size = UDim2.new(1, 0, 1, 0)

	UICorner_2.CornerRadius = UDim.new(0, 10)
	UICorner_2.Parent = Content

	TextLabel.Parent = Content
	TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.511805594, 0, 0.5, 0)
	TextLabel.Size = UDim2.new(0.976388991, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 18.000
	TextLabel.TextWrapped = true
	TextLabel.TextXAlignment = Enum.TextXAlignment.Left
	TextLabel.Text = Name

	Logo.Name = "Logo"
	Logo.Parent = Content
	Logo.AnchorPoint = Vector2.new(0.5, 0.5)
	Logo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Logo.BackgroundTransparency = 1.000
	Logo.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Logo.BorderSizePixel = 0
	Logo.Position = UDim2.new(0.910820961, 0, 0.5, 0)
	Logo.Size = UDim2.new(0.0783584565, 0, 0.875, 0)
	Logo.Image = "rbxassetid://15689064619"
	Logo.ImageColor3 = Color3.fromRGB(0, 0, 0)
	Logo.ImageTransparency = 0.250
	
	local MouseHover = false
	
	Button.Content.MouseEnter:Connect(function()
		MouseHover = true
		TweenService:Create(Button.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Rotation = math.random(-3, 3), Size = UDim2.new(1.05, 0, 1.05, 0), BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
	end)

	Button.Content.MouseLeave:Connect(function()
		MouseHover = false
		TweenService:Create(Button.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Rotation = 0, Size = UDim2.new(1, 0, 1, 0), BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.8}):Play()
	end)
	
	UIS.InputBegan:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
			TweenService:Create(Button.Content, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0.95, 0, 0.95, 0), BackgroundTransparency = 0.6}):Play()
			func()
		end
	end)
	
	UIS.InputEnded:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
			TweenService:Create(Button.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Size = UDim2.new(1, 0, 1, 0), BackgroundTransparency = 0.8}):Play()
		end
	end)
end

GUI.CreateToggle = function(Name, func)
	-- Instances:

	local Toggle = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local Content = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local TextLabel = Instance.new("TextLabel")
	local Outline = Instance.new("Frame")
	local Ball = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local Background = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local UICorner_5 = Instance.new("UICorner")
	local UIStroke = Instance.new("UIStroke")

	--Properties:

	Toggle.Name = "Toggle"
	Toggle.Parent = CurrentContent
	Toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Toggle.BackgroundTransparency = 1.000
	Toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Toggle.BorderSizePixel = 0
	Toggle.Position = UDim2.new(0.325737268, 0, 6.07363333e-08, 0)
	Toggle.Size = UDim2.new(0, 280, 0, 60)

	UICorner.CornerRadius = UDim.new(0, 10)
	UICorner.Parent = Toggle

	Content.Name = "Content"
	Content.Parent = Toggle
	Content.AnchorPoint = Vector2.new(0.5, 0.5)
	Content.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Content.BackgroundTransparency = 0.800
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(0.5, 0, 0.5, 0)
	Content.Size = UDim2.new(1, 0, 1, 0)

	UICorner_2.CornerRadius = UDim.new(0, 10)
	UICorner_2.Parent = Content

	TextLabel.Parent = Content
	TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.511805594, 0, 0.5, 0)
	TextLabel.Size = UDim2.new(0.976388991, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 18.000
	TextLabel.TextWrapped = true
	TextLabel.TextXAlignment = Enum.TextXAlignment.Left
	TextLabel.Text = Name

	Outline.Name = "Outline"
	Outline.Parent = Content
	Outline.AnchorPoint = Vector2.new(0.5, 0.5)
	Outline.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Outline.BackgroundTransparency = 1.000
	Outline.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Outline.BorderSizePixel = 0
	Outline.Position = UDim2.new(0.896527767, 0, 0.499999881, 0)
	Outline.Size = UDim2.new(0.154166669, 0, 0.799999952, 0)

	UIStroke.Name = "UIStroke"
	UIStroke.Parent = Outline
	UIStroke.Color = Color3.fromRGB(255, 255, 255)
	UIStroke.Thickness = 2
	UIStroke.Transparency = 0.45

	Ball.Name = "Ball"
	Ball.Parent = Outline
	Ball.AnchorPoint = Vector2.new(0.5, 0.5)
	Ball.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Ball.BackgroundTransparency = 0.250
	Ball.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Ball.BorderSizePixel = 0
	Ball.Position = UDim2.new(0.236000001, 0, 0.5, 0)
	Ball.Size = UDim2.new(0.422094226, 0, 0.910937548, 0)

	UICorner_3.CornerRadius = UDim.new(1, 0)
	UICorner_3.Parent = Ball

	Background.Name = "Background"
	Background.Parent = Outline
	Background.AnchorPoint = Vector2.new(0.5, 0.5)
	Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Background.BackgroundTransparency = 0.600
	Background.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Background.BorderSizePixel = 0
	Background.Position = UDim2.new(0.5, 0, 0.5, 0)
	Background.Size = UDim2.new(1, 0, 1, 0)
	Background.ZIndex = 0

	UICorner_4.CornerRadius = UDim.new(1, 0)
	UICorner_4.Parent = Background

	UICorner_5.CornerRadius = UDim.new(1, 0)
	UICorner_5.Parent = Outline
	
	local MouseHover = false
	local CanToggle = false

	Toggle.Content.MouseEnter:Connect(function()
		MouseHover = true
		TweenService:Create(Toggle.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Rotation = math.random(-3, 3), Size = UDim2.new(1.05, 0, 1.05, 0), BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
	end)

	Toggle.Content.MouseLeave:Connect(function()
		MouseHover = false
		TweenService:Create(Toggle.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Rotation = 0, Size = UDim2.new(1, 0, 1, 0), BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.8}):Play()
	end)

	UIS.InputBegan:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
			TweenService:Create(Toggle.Content, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0.95, 0, 0.95, 0), BackgroundTransparency = 0.6}):Play()
			func(CanToggle)
			
			if not CanToggle then
				CanToggle = true
				TweenService:Create(Ball, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Position = UDim2.new(0.767, 0, 0.5, 0), BackgroundTransparency = 0, BackgroundColor3 = Color3.fromRGB(149, 255, 49)}):Play()
				TweenService:Create(UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Transparency = 0, Color = Color3.fromRGB(255, 255, 255)}):Play()
			else
				CanToggle = false
				TweenService:Create(Ball, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Position = UDim2.new(0.236, 0, 0.5, 0), BackgroundTransparency = 0.25, BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
				TweenService:Create(UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Transparency = 0.45, Color = Color3.fromRGB(255, 255, 255)}):Play()
			end
			
		end
	end)

	UIS.InputEnded:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
			TweenService:Create(Toggle.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Size = UDim2.new(1, 0, 1, 0), BackgroundTransparency = 0.8}):Play()
		end
	end)
end

GUI.CreateSlider = function(Name, MinimunValue, MaximumValue, DefaultValue, func)
	-- Instances:

	local Slider = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local Content = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local TextLabel = Instance.new("TextLabel")
	local Slider_2 = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local Fill = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local OutputValue = Instance.new("TextLabel")
	local UICorner_5 = Instance.new("UICorner")

	--Properties:

	Slider.Name = Name
	Slider.Parent = CurrentContent
	Slider.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Slider.BackgroundTransparency = 1.000
	Slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Slider.BorderSizePixel = 0
	Slider.Position = UDim2.new(0.325737268, 0, 6.07363333e-08, 0)
	Slider.Size = UDim2.new(0, 280, 0, 60)

	UICorner.CornerRadius = UDim.new(0, 10)
	UICorner.Parent = Slider

	Content.Name = "Content"
	Content.Parent = Slider
	Content.AnchorPoint = Vector2.new(0.5, 0.5)
	Content.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Content.BackgroundTransparency = 0.800
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(0.5, 0, 0.5, 0)
	Content.Size = UDim2.new(1, 0, 1, 0)

	UICorner_2.CornerRadius = UDim.new(0, 10)
	UICorner_2.Parent = Content

	TextLabel.Parent = Content
	TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.511805594, 0, 0.5, 0)
	TextLabel.Size = UDim2.new(0.976388991, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 18.000
	TextLabel.TextWrapped = true
	TextLabel.TextXAlignment = Enum.TextXAlignment.Left
	TextLabel.Text = Name

	Slider_2.Name = "Slider"
	Slider_2.Parent = Content
	Slider_2.AnchorPoint = Vector2.new(0.5, 0.5)
	Slider_2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Slider_2.BackgroundTransparency = 0.600
	Slider_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Slider_2.BorderSizePixel = 0
	Slider_2.ClipsDescendants = true
	Slider_2.Position = UDim2.new(0.699999988, 0, 0.497395754, 0)
	Slider_2.Size = UDim2.new(0.361111104, 0, 0.543749988, 0)

	UICorner_3.CornerRadius = UDim.new(1, 0)
	UICorner_3.Parent = Slider_2

	Fill.Name = "Fill"
	Fill.Parent = Slider_2
	Fill.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Fill.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Fill.BorderSizePixel = 0
	Fill.Size = UDim2.new(1, 0, 1.00000012, 0)
	Fill.ClipsDescendants = true

	UICorner_4.CornerRadius = UDim.new(1, 0)
	UICorner_4.Parent = Fill

	OutputValue.Name = "OutputValue"
	OutputValue.Parent = Content
	OutputValue.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	OutputValue.BackgroundTransparency = 0.700
	OutputValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
	OutputValue.BorderSizePixel = 0
	OutputValue.Position = UDim2.new(0.905555546, 0, 0.142187595, 0)
	OutputValue.Size = UDim2.new(0.0611111112, 0, 0.699999809, 0)
	OutputValue.Font = Enum.Font.SourceSansBold
	OutputValue.Text = "14"
	OutputValue.TextColor3 = Color3.fromRGB(255, 255, 255)
	OutputValue.TextSize = 14.000
	OutputValue.TextTransparency = 0.300

	UICorner_5.CornerRadius = UDim.new(0, 6)
	UICorner_5.Parent = OutputValue
	
	local Mouse = game.Players.LocalPlayer:GetMouse()
	local Slider2 = Slider_2
	local Fill = Fill
	local Label = OutputValue

	local MaxValue = MaximumValue / 100
	local StartValue = MinimunValue / 100
	
	local SliderActive = false
	local MouseInSlider = false

	local SetValue = DefaultValue / 100
	Label.Text = tostring(math.round(SetValue * 100))

	local FinalValue = math.round(SetValue + 100)
	
	Fill.Size = UDim2.fromScale((SetValue - StartValue) * (1 - 0) / (MaxValue - StartValue) + 0, 1)

	Slider2.MouseEnter:Connect(function()
		MouseInSlider = true
	end)

	Slider2.MouseLeave:Connect(function()
		MouseInSlider = false
	end)
	
	Slider.MouseEnter:Connect(function()
		TweenService:Create(Slider2.Parent, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(255, 255, 255), BackgroundTransparency = 0.6}):Play()
	end)
	
	Slider.MouseLeave:Connect(function()
		TweenService:Create(Slider2.Parent, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.8}):Play()
	end)

	local UpdateSlider = function()
		local Output = math.clamp(((Vector2.new(Mouse.X, Mouse.Y) - Slider2.AbsolutePosition) / Slider2.AbsoluteSize).X, 0, 1)

		local OutputClamped = StartValue + (Output * (MaxValue - StartValue))

		if Output ~= OutputClamped then
			TweenService:Create(Fill, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.fromScale(Output, 1)}):Play()
		end

		SetValue = OutputClamped

		Label.Text = tostring(math.round(SetValue * 100))

		FinalValue = math.round(OutputClamped * 100)

		func(FinalValue)
	end

	local ActiveSlider = function()
		SliderActive = true
		while SliderActive do
			UpdateSlider()
			task.wait()
		end
	end

	UIS.InputBegan:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseInSlider then
			ActiveSlider()
		end
	end)

	UIS.InputEnded:Connect(function(Key)
		if Key.UserInputType == Enum.UserInputType.MouseButton1 then
			SliderActive = false
		end
	end)
end

GUI.ChangeScale = function(Scale)
	MainUI.Parent.Parent:FindFirstChildWhichIsA("UIScale").Scale = Scale
end

return GUI

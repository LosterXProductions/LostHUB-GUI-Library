	Content.BorderSizePixel = 0
	Content.ClipsDescendants = true
	Content.Position = UDim2.new(0.636022508, 0, 0.548333347, 0)
	Content.Size = UDim2.new(0, 373, 0, 259)

	UICorner_2.CornerRadius = UDim.new(0, 12)
	UICorner_2.Parent = Content

	About.Name = "About"
	About.Parent = Content

	MinBTN.Font = Enum.Font.SourceSansBold
	MinBTN.Text = "-"
	MinBTN.TextColor3 = Color3.fromRGB(255, 255, 255)
	MinBTN.TextSize = 32.000
	MinBTN.TextStrokeTransparency = 0.000

	UICorner_6.CornerRadius = UDim.new(0, 12)
	UICorner_6.Parent = DropShadow

	MouseHoverPart.Name = "MouseHoverPart"
	MouseHoverPart.Parent = DropShadow
	MouseHoverPart.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	MouseHoverPart.BackgroundTransparency = 1.000
	MouseHoverPart.BorderColor3 = Color3.fromRGB(0, 0, 0)
	MouseHoverPart.BorderSizePixel = 0
	MouseHoverPart.Position = UDim2.new(-0.0131328646, 0, -0.0166666675, 0)
	MouseHoverPart.Size = UDim2.new(0, 473, 0, 32)

	UIScale.Parent = LostHUB

	-- Scripts:

	local function QBQWN_fake_script() -- LostHUB.Manager 
		local script = Instance.new('LocalScript', LostHUB)

		wait(1)

		local ContentFolder = script.Parent.DropShadow.MainFrame.Content
		local CatagoryFolder = script.Parent.DropShadow.MainFrame.Catagory

		local TweenService = game:GetService("TweenService")
		local UIS = game:GetService("UserInputService")
		local RS = game:GetService("RunService")

		local AllowedToSwitchFrames = true
		local Clicking = false
		local HoveringFrame = false

		script.Parent.DropShadow.MainFrame.ExitBTN.MouseButton1Up:Connect(function()
			for i, v in pairs(script.Parent:GetDescendants()) do
				if v:IsA('GuiObject') then
					if v:IsA('Frame') or v:IsA('TextButton') or v:IsA('TextBox') then
						TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In, 0, false, 0), {BackgroundTransparency = 1}):Play()
					end
					if v:IsA('ImageLabel') or v:IsA('ImageButton') then
						TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In, 0, false, 0), {ImageTransparency = 1}):Play()
					end
					if v:IsA('TextLabel') or v:IsA('TextButton') or v:IsA('TextBox') then
						TweenService:Create(v, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In, 0, false, 0), {TextTransparency = 1, TextStrokeTransparency = 1}):Play()
					end
				end
			end
			wait(0.6)
			script.Parent:Destroy()
		end)

		local SwitchFrames = function(Button, v)
			if v.Position == UDim2.new(0.5, 0, 0.5, 0) and v.Name ~= Button.Name then
				TweenService:Create(v, TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(-0.5, 0, 0.5, 0)}):Play()
			end

			if v.Name == Button.Name then
				v.Position = UDim2.new(1.5, 0, 0.5, 0)
				TweenService:Create(v, TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(0.5, 0, 0.5, 0)}):Play()
			end
		end

		local function UpdateCanvasSize(Canvas)
			Canvas.CanvasSize = UDim2.new(0, 0, 0, Canvas.UIGridLayout.AbsoluteContentSize.Y)
		end

		for i, v in pairs(ContentFolder:GetChildren()) do
			if v:IsA("ScrollingFrame") then
				UpdateCanvasSize(v)
			end
		end

		for i, v in pairs(CatagoryFolder:GetChildren()) do
			if v:IsA("ScrollingFrame") then
				UpdateCanvasSize(v)
			end
		end

		script.Parent.DropShadow.MouseHoverPart.MouseEnter:Connect(function()
			HoveringFrame = true
		end)

		script.Parent.DropShadow.MouseHoverPart.MouseLeave:Connect(function()
			HoveringFrame = false
		end)

		UIS.InputBegan:Connect(function(Key)
			if Key.UserInputType == Enum.UserInputType.MouseButton1 and HoveringFrame then
				Clicking = true
			end
		end)

		UIS.InputEnded:Connect(function(Key)
			if Key.UserInputType == Enum.UserInputType.MouseButton1 then
				Clicking = false
			end
		end)

		for i, Button in pairs(CatagoryFolder.Buttons:GetChildren()) do
			if Button:IsA("Frame") then
				local MouseHover = false

				Button.Content.MouseEnter:Connect(function()
					MouseHover = true
					TweenService:Create(Button.Content, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Rotation = math.random(-17, 17), Size = UDim2.new(1.05, 0, 1.05, 0), BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
				end)

				Button.Content.MouseLeave:Connect(function()
					MouseHover = false
					TweenService:Create(Button.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Rotation = 0, Size = UDim2.new(1, 0, 1, 0), BackgroundColor3 = Color3.fromRGB(0, 0, 0), BackgroundTransparency = 0.8}):Play()
				end)

				UIS.InputBegan:Connect(function(Key)
					if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
						TweenService:Create(Button.Content, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0.95, 0, 0.95, 0), BackgroundTransparency = 0.6}):Play()
						if AllowedToSwitchFrames then
							AllowedToSwitchFrames = false
							for i, v in pairs(ContentFolder:GetChildren()) do
								if ContentFolder:FindFirstChild(Button.Name).Position ~= UDim2.new(0.5, 0, 0.5, 0) then
									if v:IsA("ScrollingFrame") then
										if v.Position == UDim2.new(0.5, 0, 0.5, 0) and v.Name ~= Button.Name then
											TweenService:Create(v, TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(-0.5, 0, 0.5, 0)}):Play()
										end
									end

									if v.Name == Button.Name then
										v.Position = UDim2.new(1.5, 0, 0.5, 0)
										TweenService:Create(v, TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(0.5, 0, 0.5, 0)}):Play()
									end
								end

							end
							wait(0.6)
							AllowedToSwitchFrames = true
						end
					end
				end)

				UIS.InputEnded:Connect(function(Key)
					if Key.UserInputType == Enum.UserInputType.MouseButton1 and MouseHover then
						TweenService:Create(Button.Content, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Size = UDim2.new(1, 0, 1, 0), BackgroundTransparency = 0.8}):Play()
					end
				end)
			end
		end

		RS.RenderStepped:Connect(function()
			if Clicking then
				TweenService:Create(script.Parent.DropShadow, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Position = UDim2.new(0, game.Players.LocalPlayer:GetMouse().X, 0, game.Players.LocalPlayer:GetMouse().Y + 140)}):Play()
			end
		end)
	end
	coroutine.wrap(QBQWN_fake_script)()
	
	MainUI = MainFrame
end

GUI.CreateCatagory = function(Text)
	-- Instances:

	local CatagoryBTN = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local Content = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local TextLabel = Instance.new("TextLabel")

	--Properties:

	CatagoryBTN.Name = Text
	CatagoryBTN.Parent = MainUI.Catagory.Buttons
	CatagoryBTN.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	CatagoryBTN.BackgroundTransparency = 1.000
	CatagoryBTN.BorderColor3 = Color3.fromRGB(0, 0, 0)
	CatagoryBTN.BorderSizePixel = 0
	CatagoryBTN.Size = UDim2.new(0, 100, 0, 100)

	UICorner.CornerRadius = UDim.new(0, 10)
	UICorner.Parent = CatagoryBTN

	Content.Name = "Content"
	Content.Parent = CatagoryBTN
	Content.AnchorPoint = Vector2.new(0.5, 0.5)
	Content.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Content.BackgroundTransparency = 0.800
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(0.5, 0, 0.5, 0)
	Content.Size = UDim2.new(1, 0, 1, 0)

	UICorner_2.CornerRadius = UDim.new(0, 10)
	UICorner_2.Parent = Content

	TextLabel.Parent = Content
	TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BorderSizePixel = 0
	TextLabel.Position = UDim2.new(0.5, 0, 0.5, 0)
	TextLabel.Size = UDim2.new(1, 0, 1, 0)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.Text = Text
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 18.000
	
	-- Instances:

	local Content = Instance.new("ScrollingFrame")
	local UIGridLayout = Instance.new("UIGridLayout")

	--Properties:

	Content.Name = Text
	Content.Parent = MainUI.Content
	Content.Active = true
	Content.AnchorPoint = Vector2.new(0.5, 0.5)
	Content.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Content.BackgroundTransparency = 1.000
	Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Content.BorderSizePixel = 0
	Content.Position = UDim2.new(1.5, 0, 0.5, 0)
	Content.Size = UDim2.new(1, 0, 0.970000029, 0)
	Content.CanvasSize = UDim2.new(0, 0, 1, 0)
	Content.ScrollBarThickness = 4

	UIGridLayout.Parent = Content
	UIGridLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIGridLayout.CellPadding = UDim2.new(0, 5, 0, 3)
	UIGridLayout.CellSize = UDim2.new(0, 360, 0, 32)
	
	CurrentContent = Content
end

